# OpenRouter配置 (用于POET评估中的LLM Judge)
OPENAI_API_KEY=your_openrouter_api_key_here
OPENAI_BASE_URL=https://openrouter.ai/api/v1
OPENAI_MODEL=google/gemini-2.5-pro


# Jina配置 (用于网页抓取)
JINA_API_KEY=your_jina_api_key_here

# POET评估配置
POET_DEFAULT_EXPERT_TIME_HOURS=2.0
POET_DEFAULT_EXPERT_RATE_USD=100.0
POET_KNOWLEDGE_BASE_PATH=poet_knowledge_base.json
POET_CONFIG_PATH=config/poet_config.json

# 评估并发配置
MAX_WORKERS=4
EVALUATION_TIMEOUT_SECONDS=300

# 结果输出配置
RESULTS_OUTPUT_DIR=results/poet
ENABLE_DETAILED_LOGGING=true
